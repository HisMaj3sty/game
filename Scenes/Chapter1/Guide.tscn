[sub_resource type="GDScript" id=1]
script/source = "extends Node2D


func get_dialog_number() -> int:
	if GLOBAL.loaded != \"-1\":
		var fac = PROP.get(\"facility\")
		if fac == \"0\":
			return 5
		if fac == \"1\":
			return 6
		if fac == \"2\":
			return 7
	NOTIFY.show(\"NOTLOADED\")
	return -1


func activate() -> void:
	DIALOG.show_dialog(get_dialog_number())
"

